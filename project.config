[access]
	inheritFrom = Private-Projects
[access "refs/*"]
	owner = group user/Andy Dufilie (adufilie)
[access "refs/for/refs/*"]
	read = group WeaveTeam
	read = group user/Andy Dufilie (adufilie)
	push = group WeaveTeam
	push = group user/Andy Dufilie (adufilie)
	label-Code-Review = -2..+2 group WeaveTeam
	label-Code-Review = -2..+2 group user/Andy Dufilie (adufilie)
	label-Verified = -1..+1 group WeaveTeam
	label-Verified = -1..+1 group user/Andy Dufilie (adufilie)
	rebase = group WeaveTeam
	rebase = group user/Andy Dufilie (adufilie)
	removeReviewer = group WeaveTeam
	removeReviewer = group user/Andy Dufilie (adufilie)
	submit = group WeaveTeam
	submit = group user/Andy Dufilie (adufilie)
[access "refs/heads/*"]
	read = group WeaveTeam
	read = group user/Andy Dufilie (adufilie)
	create = group WeaveTeam
	create = group user/Andy Dufilie (adufilie)
	push = group WeaveTeam
	push = +force group user/Andy Dufilie (adufilie)
	pushMerge = group WeaveTeam
	pushMerge = group user/Andy Dufilie (adufilie)
[access "refs/tags/*"]
	push = group WeaveTeam
	push = +force group user/Andy Dufilie (adufilie)
	pushTag = group WeaveTeam
	pushTag = group user/Andy Dufilie (adufilie)
	pushSignedTag = group WeaveTeam
	pushSignedTag = group user/Andy Dufilie (adufilie)
