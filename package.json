{
  "name": "weavejs",
  "version": "0.0.0",
  "description": "Collection of visualization tools and UI for the WeaveJS project.",
  "main": "lib/weavejs.js",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "clean": "npm run clean:outts && npm run clean:lib && npm run clean:dist",
    "clean:outts": "rimraf outts/ && rimraf scripts/outts/",
    "clean:lib": "rimraf lib/*.js",
    "clean:dist": "rimraf dist/*.js dist/*.css dist/*.html",
    "copy": "node run-ts copy",
    "dts": "tsc -p tsconfig-definitions.json",
    "compile": "npm run compile-as && npm run compile-dts && npm run compile-ts && npm run copy",
    "compile-refs": "node run-ts main-import-tool -d srcts/weavejs -f srcts/_references.ts && npm run compile-librefs",
    "compile-librefs": "node run-ts main-import-tool -d typings/ -f srcts/_lib-references.ts",
    "compile-libs": "npm run compile-librefs && tsc -p tsconfig-libs.json && babel outts/libs.js --out-file lib/libs.js && browserifyinc --cachefile  browserifycache.json lib/libs.js > dist/libs.js",
    "compile-module": "tsc -p tsconfig-module.json && babel outts/module.js --out-file lib/module.js",
    "compile-dts": "as2dts --defs-only ./WeaveASJS/src ./WeaveASJS/typings/src && tsc -d -p ./WeaveASJS/typings/ && mv WeaveASJS/typings/*.d.ts ./typings/weave/",
    "compile-ts": "npm run compile-refs && tsc -p tsconfig.json && babel outts/weavejs.js --out-file dist/weavejs.js",
    "compile-as": "node WeaveASJS/build.js",
    "postinstall": "cd lib || ( npm run compile && npm run compile-module && npm run dts )"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WeaveTeam/WeaveJS.git"
  },
  "keywords": [
    "visualization",
    "weave"
  ],
  "author": "WeaveTeam <devteam@iweave.com>",
  "license": "MPL-2.0",
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "browserify": "^12.0.1",
    "flexjs": "https://github.com/WeaveTeam/flex-utilities.git#npm-flexjs-only",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.5",
    "minifyify": "^7.1.0",
    "minimist": "^1.2.0",
    "tsd": "^0.6.5",
    "tsort": "^0.0.1"
  },
  "dependencies": {
    "as2dts": "^0.2.3",
    "babel": "^6.1.18",
    "babel-cli": "^6.4.5",
    "browserify-incremental": "^3.1.1",
    "c3": "https://github.com/WeaveTeam/c3.git",
    "classnames": "^2.2.3",
    "clipboard": "^1.5.10",
    "codemirror": "^5.17.0",
    "d3": "^3.5.9",
    "filesaver.js": "^0.2.0",
    "fixed-data-table": "^0.6.0",
    "font-awesome": "^4.4.0",
    "jquery": "^2.1.4",
    "jszip": "^3.0.0",
    "lodash": "^3.10.1",
    "moment": "^2.11.2",
    "openlayers": "3.13.0",
    "pixi.js": "^3.0.11",
    "proj4": "^2.3.14",
    "rc-slider": "^3.6.2",
    "react": "0.14.7",
    "react-addons-update": "0.14.7",
    "react-codemirror": "^0.2.6",
    "react-color": "^2.0.0",
    "react-dom": "0.14.7",
    "react-dropzone": "^3.4.0",
    "react-notification-system": "^0.2.7",
    "react-sparklines": "^1.5.2",
    "rimraf": "^2.5.3",
    "typescript": "^1.8.7"
  }
}
