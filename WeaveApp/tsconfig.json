{
  "compilerOptions": {
    "noImplicitAny": true,
    "jsx": "preserve",
    "removeComments": false,
    "target": "es6",
    "module": "es6",
	"outDir": "./bin/es6/",
    "inlineSources": true,
    "inlineSourceMap": true,
	"forceConsistentCasingInFileNames": true,
	"listFiles": false,
	"noImplicitReturns": false,
	"pretty": false,
	"newLine": "lf",
	"baseUrl": "./src"
  },
  "compileOnSave": false
}
